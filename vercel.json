{
  "name": "platzivideo-secured",
  "alias": ["platzivideo-secured-movies-api"],
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@platzivideos_db_user",
    "DB_PASSWORD": "@platzivideos_db_password",
    "DB_HOST": "@platzivideos_db_host",
    "DB_NAME": "@platzivideos_db_name",
    "AUTH_JWT_SECRET": "@platzivideo_auth_jwt_secret",
    "PUBLIC_API_KEY_TOKEN": "@platzivideo_public_api_key_token",
    "ADMIN_API_KEY_TOKEN": "@platzivideo_admin_api_key_token"
  }
}
